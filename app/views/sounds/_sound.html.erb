<div id="soundBoxContainer">
  <div id="soundBoxInnerContainer">


<div class='soundBox'>
  <audio autoplay controls="true" loop="true" src="<%= sound.url %>" class="mutetabId_2442606">
      Your browser doesn't support the audio element.
  </audio>
    <div class="controls">
      <p>delayTime: <input max="1" min="0" name="<%= delayTime %>" step="0.05" type="range" value="0"></p>
      <p>feedback: <input max="0.95" min="0" name="<%= feedback %>" step="0.05" type="range" value="0"></p>
      <p>cutoff freq: <input max="4000" min="0" name="<%= frequency%>" step="100" type="range" value="1000"></p>
      <p>Reverb: <input max="3" min="1" name="<%= reverb %>" step=".05" type="range" value="0"></p>
      <p>Reverb Volume: <input max="6" min="0" name="<%= reverbGain %>" step=".05" type="range" value="0"></p>
      <p>LowPass Filter: <input max="22050" min="1000" name="<%= filter %>" step="200" type="range" value="6400"></p>
      <p>LowPass Filter Volume: <input max="5" min="0" name="<%= filterGain %>" step=".05" type="range" value="0"></p>
    </div>
</div>
</div>
</div>
